$(document).ready(function () {
    var profe = $('#profe');
    $('#b').click(function () {
        $.ajax({
            type: 'GET',
            url: "api/predmet",
            dataType: "json",
            success: function (data) {
                profe.empty();
                $.each(data, function (index, val) {
                    profe.append('<li>' + val.Naziv_predmeta + '</li>' + '<input id="' + val.id + '"' + ' type="button" class="predmetDetalji" value="detalji" />')
                });
            }
        });
    });
});